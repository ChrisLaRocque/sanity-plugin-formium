{
  "name": "sanity-plugin-formium",
  "version": "1.0.0",
  "description": "A Sanity plugin for Formium forms",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/ChrisLaRocque/sanity-plugin-formium#readme",
  "bugs": {
    "url": "https://github.com/ChrisLaRocque/sanity-plugin-formium/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/ChrisLaRocque/sanity-plugin-formium.git"
  },
  "license": "MIT",
  "author": "Chris LaRocque <chris.larocque@sanity.io>",
  "sideEffects": false,
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./lib/index.mjs",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.esm.js",
  "types": "./lib/index.d.ts",
  "files": [
    "src",
    "lib",
    "v2-incompatible.js",
    "sanity.json"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "test": "echo 'no tests'",
    "watch": "pkg-utils watch --strict"
  },
  "browserslist": "extends @sanity/browserslist-config",
  "prettier": "@sanity/prettier-config",
  "dependencies": {
    "@formium/client": "^0.1.4",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/studio-secrets": "^3.0.0",
    "@sanity/ui": "^2.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@sanity/eslint-config-studio": "^4.0.0",
    "@sanity/pkg-utils": "^6.7.0",
    "@sanity/plugin-kit": "^4.0.4",
    "@sanity/prettier-config": "^1.0.2",
    "@sanity/semantic-release-preset": "^4.1.7",
    "@types/react": "^18.2.79",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sanity": "^7.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sanity": "^3.38.0",
    "semantic-release": "^23.0.8",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3",
    "styled-components": "^6.1"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
